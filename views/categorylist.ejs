<%- include('components/head') %>
<%- include('components/header') %>
<div class="container">
  <div class="row">
    <table class="table table-hover table-bordered">
      <thead>
      <tr>
        <th>名字</th>
        <th>时间</th>
        <th>查看</th>
        <th>修改</th>
        <th>删除</th>
      </tr>
      <tbody>
      <% categories.forEach((item)=>{ %>
      <tr class="item-id-<%= item._id %>">
        <td><%= item.name %></td>
        <td><%= moment(item.meta.createAt).format('YYYY/MM/DD')%></td>
        <td><a href="../movie/<%= item._id %>" target="_blank">查看</a></td>
        <td><a href="../admin/update/<%= item._id %>" target="_blank">修改</a></td>
        <td>
          <button class="btn btn-danger del" type="button" data-id="<% item._id %>">删除</button>
        </td>
      </tr>
      <% }) %>
      </tbody>
      </thead>
    </table>
  </div>
</div>
</body>
</html>